{
    "name": "ferret-server-ui",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "generate": "rimraf src/models/api && openapi-generator generate -Dmodels --skip-validate-spec -i api/api.oas2.json -g typescript-inversify -o src/models/api && tslint --fix --project ./tsconfig.json -c tslint.json src/models/api/**/*.ts",
        "build": "npm run generate && react-scripts build",
        "test": "react-scripts test"
    },
    "dependencies": {
        "antd": "^3.16.6",
        "apollo-cache-inmemory": "^1.5.1",
        "apollo-client": "^2.5.1",
        "apollo-link": "^1.2.11",
        "apollo-link-context": "^1.0.17",
        "apollo-link-rest": "^0.7.3",
        "classnames": "^2.2.6",
        "graphql": "^14.2.1",
        "graphql-anywhere": "^4.2.1",
        "graphql-tag": "^2.10.1",
        "history": "^4.9.0",
        "lodash": "^4.17.11",
        "qs": "^6.7.0",
        "react": "^16.8.6",
        "react-apollo": "^2.5.5",
        "react-dom": "^16.8.6",
        "react-router": "^5.0.0",
        "react-router-breadcrumbs-hoc": "^3.2.0",
        "react-router-dom": "^5.0.0",
        "react-transition-group": "^4.0.0"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^0.0.8-4.0.0-beta2",
        "@types/classnames": "^2.2.7",
        "@types/graphql": "^14.2.0",
        "@types/jest": "24.0.11",
        "@types/lodash": "^4.14.123",
        "@types/node": "11.11.5",
        "@types/qs": "^6.5.2",
        "@types/react": "16.8.16",
        "@types/react-dom": "16.8.4",
        "@types/react-router": "^4.4.5",
        "@types/react-router-dom": "^4.3.2",
        "@types/react-transition-group": "^2.9.1",
        "http-proxy-middleware": "^0.19.1",
        "husky": "^2.2.0",
        "node-sass": "^4.12.0",
        "prettier": "^1.17.0",
        "pretty-quick": "^1.10.0",
        "react-scripts": "2.1.8",
        "rimraf": "^2.6.3",
        "tslint": "^5.16.0",
        "tslint-config-airbnb": "^5.11.1",
        "typescript": "3.4.5"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
